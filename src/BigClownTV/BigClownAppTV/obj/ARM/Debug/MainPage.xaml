                    <charts:SfChart Header="{Binding Path=GraphHeader}">
                       
                        <charts:SfChart.PrimaryAxis>
                            <charts:DateTimeAxis IntervalType="Hours" Header="Time"/>
                        </charts:SfChart.PrimaryAxis>

                        <charts:SfChart.SecondaryAxis>
                            <charts:NumericalAxis StartRangeFromZero="False" OpposedPosition="True" RangePadding="Normal" LabelFormat="##.##" Header="{Binding Path=GraphValue}"/>
                        </charts:SfChart.SecondaryAxis>

                        <charts:FastLineSeries ItemsSource="{Binding Path=UnitCollection}" Interior="Red"  XBindingPath="Time" YBindingPath="Value"/>


                    </charts:SfChart>
                </Viewbox>
            </Grid>
        </SplitView>
    </SplitView>
</Page>




